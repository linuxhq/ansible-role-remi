# This repository only provides GLPI 9.2 and its plugins
# NOTICE: you need at least PHP 5.6

[remi-glpi92]
name=Remi's GLPI 9.2 RPM repository for Enterprise Linux {{ remi_rel }} - $basearch
#baseurl=http://rpms.remirepo.net/enterprise/{{ remi_rel }}/glpi92/$basearch/
#mirrorlist=https://rpms.remirepo.net/enterprise/{{ remi_rel }}/glpi92/httpsmirror
mirrorlist=http://cdn.remirepo.net/enterprise/{{ remi_rel }}/glpi92/mirror
enabled={{ '1' if remi_repository_remi_glpi92|bool else '0' }}
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-remi

