# This repository only provides GLPI 9.3 and its plugins
# NOTICE: you need at least PHP 5.6

[remi-glpi93]
name=Remi's GLPI 9.3 RPM repository for Enterprise Linux {{ remi_rel }} - $basearch
#baseurl=http://rpms.remirepo.net/enterprise/{{ remi_rel }}/glpi93/$basearch/
#mirrorlist=https://rpms.remirepo.net/enterprise/{{ remi_rel }}/glpi93/httpsmirror
mirrorlist=http://cdn.remirepo.net/enterprise/{{ remi_rel }}/glpi93/mirror
enabled={{ '1' if remi_repository_remi_glpi93|bool else '0' }}
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-remi

